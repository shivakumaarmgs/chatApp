<div class="container">
  <div class="jumbotron">
    <center>
      <h1>@ Connect</h1>
    </center>
    <form action='/chatroom/create' method='post' class="form-group">
      <div class="form-group col-lg-6 col-lg-offset-3">
        <div class="form-group col-lg-12">
          <input class="form-control" type="text" name="roomName" placeholder="Type in a room name"></input>
        </div>
        <div class="form-group col-lg-4 col-lg-offset-2">
          <input class="btn btn-primary btn-md" type="submit" value="Create Room"></input>
        </div>
        <div class="form-group col-lg-4">
          <input class="btn btn-primary btn-md" type="submit" value="Join Room" onclick=joinRoom(event)></input>
        </div>
      </div>
    </form>
  </div>
</div>
